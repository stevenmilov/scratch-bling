    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Flask run",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Flask init",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "initdb"
            ],
            "jinja": true
        },
        {
            "name": "Flask bootstrap",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "bootstrap"
            ],
            "jinja": true
        }
        
    ]
}